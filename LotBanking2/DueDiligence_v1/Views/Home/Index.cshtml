<link href="@Url.Content("~/Content/bargraph.css")" rel="stylesheet" type="text/css" />

@{
    ViewBag.Title = "Crescent Bay Holdings";
  
}

<body>
    <div class="menubar">
        <div class="table">
            <ul id="horizontal-list">
                <li>DOCUMENTATION</li>
                <li>PERFORMANCE METRICS</li>
                <li>SALES REPORT</li>
                <li>TRAFFIC REPORT</li>
                <li>LOCATION</li>
            </ul>
        </div>


    </div>
    <div class="colmask threecol">

        <div class="colmid">
            <div class="colleft">
                <div class="col1">
                    <!-- Column 2 start -->
                    <div class="position">
                        <div class="graphcontainer">
                            <div class="bar" id="bar1">
                                <div class="percent">
                                    <span style="width: 100%;"></span>
                                </div>
                                <div class="circle">
                                    <span>$0</span>
                                </div>
                                <div class="text">
                                    <input type="text" class="input" value="0" />
                                    <small>Construction Cost over Expected Cost</small>
                                </div>
                            </div>
                            <div class="bar" id="bar2">
                                <div class="percent">
                                    <span style="width: 100%;"></span>
                                </div>
                                <div class="circle">
                                    <span>$0</span>
                                </div>
                                <div class="text">
                                    <input type="text" class="input" value="0" />
                                    <small>Lots purchased over lots available</small>
                                </div>
                            </div>
                            <div class="bar" id="bar3">
                                <div class="percent">
                                    <span style="width: 100%;"></span>
                                </div>
                                <div class="circle">
                                    <span>$0</span>
                                </div>
                                <div class="text">
                                    <input type="text" class="input" value="0" />
                                    <small>Lots sold over lots available</small>
                                </div>
                            </div>
                        </div>
                        <div class="bottomhalf">
                            <div class="tscontainer">
                                <img src="http://localhost:4838/Images/delete_salesTraffic.png" height="100%" width="100%"/>
                            </div>
                            <div class="mapcontainer">
                                <img src="http://localhost:4838/Images/delete_gMaps.png"  height="100%" width="100%" />
                            </div>
                        </div>
                        <!-- Column 2 end -->
                    </div>
                </div>
                <div class="col2">
                    <!-- Column 1 start -->
                    <table class="tasklist">
                        <tr>
                            <th>TASK LIST</th>
                        </tr>
                        <tr>
                            <td>row 1, cell 1</td>
                        </tr>
                        <tr>
                            <td>row 1, cell 2</td>
                        </tr>
                    </table>


                    <!-- Column 1 end -->
                </div>
                <div class="col3">
                    <!-- Column 3 start -->
                    <div class="title">
                        Builders
                    </div>
                    <!-- Column 3 end -->
                </div>
            </div>
        </div>
    </div>
</body>
