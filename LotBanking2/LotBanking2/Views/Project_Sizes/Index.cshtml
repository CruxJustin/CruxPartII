            @Html.DisplayNameFor(model => model.setbacks_rear)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.setbacks_side)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.date_created)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.date_modified)
        </th>
        <th></th>
